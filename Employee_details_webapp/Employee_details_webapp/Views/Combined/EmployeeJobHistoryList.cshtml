@model List<EmployeeJobHistoriesModel>
@{
}
<h2>View or Edit employees job histories!</h2>
<br />
<h3>Employee Full name: @ViewBag.fullname </h3>
<h3>Employee ID: @Model[0].Employeeid</h3>
<br />
<table class="table">
	<thead>
		<tr>
			<th>Position Id</th>
			<th>Employee Job History ID</th>
			<th>Position Name</th>
			<th>Start Date</th>
			<th>End Date</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var history in Model)
		{
			<tr>
				<td>@history.Positionid</td>
				<td>@history.EmployeeJobHistoryid</td>
				<td>@history.PositionName</td>
				<td>@history.StartDate</td>
				<td>@history.EndDate</td>
				<td><a href="EditEmployee/@history.Employeeid">Edit</a></td>
			</tr>
		}
	</tbody>
</table>
